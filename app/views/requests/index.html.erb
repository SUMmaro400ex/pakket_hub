<div class="jumbotron" id="otherpages">
  <div class="container">
    <div class="row">
      <nav class="navbar navbar-default">
        <div class="container">
          <div class="navbar-header">
            <img id="logo" src="<%= asset_path("logo") %>">
          </div>
        </div>
      </nav>
    </div>
  </div>
</div>


<div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-6">
    <h2>Available Requests for your Travel Time</h2>
<% @results.each do |result| %>
<p>Longitude <%= result.location.longitude %> Latitude <%= result.location.latitude %>
  Start Date <%= result.start_date %> End Date <%= result.end_date %>
</p>    <% end %>
  </div>
</div>

<div id="invisible"><%= form_for @travel_plan, url: {controller: :travel_plans, action: :create} do |request_form| %>
  <%= request_form.text_field :radius %>
  <%= request_form.date_field :start_date %>
  <%= request_form.date_field :end_date %>
  <%= request_form.text_field :courier_id %>
  <br>
  <%= request_form.fields_for :location, @travel_plan.location || Location.new do |location_form| %>
    <%= location_form.text_field :latitude %>
    <%= location_form.text_field :longitude %>
  <% end %></div>
  <%= request_form.submit %>
<% end %>

