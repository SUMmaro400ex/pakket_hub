<div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-6">
    <h2>Begin Travel Plan</h2>

    <% @radius = 1 %>
    <% @start_date = params[:date] if params[:date]%>
    <% @end_date = params[:date] if params[:date]%>
    <% @address = params[:address] if params[:address]%>
    <%= form_for @travel_plan, url: {controller: :requests, action: :search} do |travel_plan_form| %>
    <%= travel_plan_form.label :radius %>
    <%= travel_plan_form.text_field :radius, value: @radius  %>
    <%= travel_plan_form.label :start_date %>
    <%= travel_plan_form.date_field :start_date, value: @start_date.try(:to_date) %>
    <%= travel_plan_form.label :end_date %>
    <%= travel_plan_form.date_field :end_date, value: @start_date.try(:to_date) %>
    <%= travel_plan_form.fields_for :destination, @travel_plan.destination || Location.new do |location_form| %>
     <div id="hide_me"> <%= location_form.label :latitude %>
      <%= location_form.text_field :latitude %>
      <%= location_form.label :longitude %>
      <%= location_form.text_field :longitude %> </div>
      <%= location_form.label :address %>
      <%= location_form.text_field :address, value: @address%>
      <% end %>
      <%= travel_plan_form.submit %>
    <% end %>
  </div>
  </div>


