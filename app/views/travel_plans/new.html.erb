<%= form_for @travel_plan, url: {controller: :requests, action: :search} do |travel_plan_form| %>
  <%= travel_plan_form.text_field :radius %>
  <%= travel_plan_form.text_field :start_date %>
  <%= travel_plan_form.text_field :end_date %>
  <%= travel_plan_form.fields_for :destination, @travel_plan.destination || Location.new do |location_form| %>
    <%= location_form.text_field :latitude %>
    <%= location_form.text_field :longitude %>
  <% end %>
  <%= travel_plan_form.submit %>
<% end %>