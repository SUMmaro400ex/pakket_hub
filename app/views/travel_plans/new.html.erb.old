<div class="row">
  <div class="col-md-3">
    </div>
  <div class="col-md-6">
<%= form_for(:travel_plans) do |f|%>
  <div class="form-group"> <!-- Name field -->
    <%= f.label :destination,"From", class: "control-label " %>
    <%= f.text_field :destination, class: "form-control", id: "from"%>
  </div>
  <label>Package Size</label>
  <label class="radio-inline">
    <input type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1"> S
  </label>
  <label class="radio-inline">
    <input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2"> M
  </label>
  <label class="radio-inline">
    <input type="radio" name="inlineRadioOptions" id="inlineRadio3" value="option3"> L
  </label>
  <label class="radio-inline">
    <input type="radio" name="inlineRadioOptions" id="inlineRadio3" value="option3"> XL
  </label>

  <div class="form-group"> <!-- Subject field -->
    <label class="control-label " for="subject">Travel Radius</label>
    <input class="form-control" id="subject" name="subject" type="text"/>
  </div>

  <div class="form-group"> <!-- Message field -->
    <label class="control-label " for="message">Message</label>
    <textarea class="form-control" cols="40" id="message" name="message" rows="10"></textarea>
  </div>

  <div class="form-group">
    <button class="btn btn-primary " name="submit" type="submit">Submit</button>
  </div>

<% end %>
  </div>
  <div class="col-md-3">
  </div>
    </div>
</section>

